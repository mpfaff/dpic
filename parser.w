		    }
		  }
		;

ncount	:	XLfloat                                              /* ncount1 */

		| XSLQ expression XSRQ                                   /* ncount2 */
		{ $$.xval = $2.xval; }

		| XLBRACE expression XRBRACE                             /* ncount3 */
		{ $$.xval = $2.xval; }
		;

logprod	:	logval                                              /* logprod1 */

		| logprod XANDAND logval                                /* logprod2 */
		{ if (($1.xval == 0.0) || ($3.xval == 0.0)) { $$.xval = 0.0; }

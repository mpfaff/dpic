		{ inlogic = true; }
		;

logexpr	:	logprod                                             /* logexpr1 */

		| logexpr XOROR logprod                                 /* logexpr2 */
		{ if (($1.xval != 0.0) || ($3.xval != 0.0)) { $$.xval = 1.0; }
    	  else { $$.xval = 0.0; }

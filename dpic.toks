Xlparen  "("
Xrparen  ")"
Xmult  "*"
Xplus  "+"
Xminus  "-"
Xdiv  "/"
Xpercent  "%"
XNL  "<NL>"
  =  ";"
Xcaret  "^"
XNOT  "!"
XANDAND  "&&"
